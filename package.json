{
  "name": "estil-chat",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm:cli": "cd src/db/typeorm-migrations && ts-node --project ../../../tsconfig.json -r tsconfig-paths/register ../../../node_modules/typeorm/cli -d ../../../bin/typeorm-config.ts",
    "migration:generate": "yarn typeorm:cli migration:generate",
    "migration:create": "cd src/db/typeorm-migrations && typeorm-ts-node-commonjs migration:create",
    "migration:run": "yarn typeorm:cli migration:run",
    "migration:down": "yarn typeorm:cli migration:revert",
    "mongo:migration:run": "ts-node node_modules/.bin/migrate-mongo up -f bin/mongo-config.ts",
    "mongo:migration:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f bin/mongo-config.ts' ",
    "mongo:migration:status": "ts-node node_modules/.bin/migrate-mongo status -f bin/mongo-config.ts",
    "mongo:migration:down": "ts-node node_modules/.bin/migrate-mongo down -f bin/mongo-config.ts && yarn migration:status",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^9.1.4",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.15.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "migrate-mongo": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "mongodb-memory-server": "^8.9.3",
    "mongoose": "^6.6.5",
    "nanoid": "^3.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pino": "^8.1.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.2",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
